{
  "name": "device-interception-example",
  "version": "1.1.0",
  "description": "An example Node.js application to capture keyboard and mouse strokes.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start:node": "set NODE_ENV=production && bun index.js",
    "start:bun": "set NODE_ENV=production && bun index.js",
    "serve:node": "set NODE_ENV=production && node serve.js",
    "serve:bun": "set NODE_ENV=production && bun serve.js",
    "install:driver": "npx node-interception /install",
    "uninstall:driver": "npx node-interception /uninstall",
    "check": "node check.js",
    "web": "vite",
    "build:web": "vite build",
    "serve:bun_#_web": "concurrently \"npm run serve:bun\" \"npm run web\""
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "evp-design-ui": "^0.0.13-beta",
    "express": "^4.19.2",
    "moment": "^2.30.1",
    "node-interception": "^2.0.4",
    "react": "^18.3.1",
    "react-calendar": "^5.0.0",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "vite": "^5.4.1"
  }
}
